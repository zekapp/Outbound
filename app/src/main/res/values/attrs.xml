<?xml version="1.0" encoding="utf-8"?>
<resources >

    <!-- Robot Font object Declaration -->
    <declare-styleable name="RobotoTextView">
        <attr name="typeface"/>
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="roboto_bold" value="0"/>
        <enum name="roboto_light" value="1"/>
        <enum name="roboto_medium" value="2"/>
        <enum name="roboto_regular" value="3"/>
    </attr>

    <declare-styleable name="MultiSwipeRefreshLayout">
        <attr name="foreground" format="reference" />
    </declare-styleable>

</resources>
